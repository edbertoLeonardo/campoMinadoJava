package campoMinado;

import static org.junit.jupiter.api.Assertions.*;

import org.junit.jupiter.api.Test;

class Teste {

	@Test
	void testarIgualADois() {
		
		int a = 1 + 1;   
		
		assertEquals(a, 2);
		
	}
	
	@Test
	void testarIgualATres() {
		
		int x = 2 + 10 - 9;
		assertEquals(x, 3);
	}
	

}
